{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "strict": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "lib": [
      "ESNext"
    ],
    "outDir": "./dist"
  },
  "exclude": [
    "node_modules",
    "dist"
  ],
}